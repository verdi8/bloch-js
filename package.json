{
  "name": "bloch-js",
  "version": "0.1.0",
  "description": "A javascript Bloch Sphere visualization component.",
  "keywords": [
    "bloch",
    "sphere",
    "quantum"
  ],
  "homepage": "https://github.com/verdi8/bloch-js/",
  "main": "dist/bloch.js",
  "unpkg": "dist/bloch.min.js",
  "jsdelivr": "dist/bloch.min.js",
  "module": "src/bloch.js",
  "repository": {
    "type": "git",
